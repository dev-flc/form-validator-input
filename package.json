{
  "name": "template",
  "version": "1.0.0",
  "description": "template app react",
  "main": "index.js",
  "scripts": {
    "build": "SET VERSION_APP=%npm_package_version%& SET NAME_APP=%npm_package_name%& webpack --config webpack/webpack.prod.js",
    "buildd": "NAME_APP=$npm_package_name VERSION_APP=$npm_package_version webpack --config webpack/webpack.prod.js",
    "de": "NAME_APP=$npm_package_name VERSION=$npm_package_version webpack-dev-server --progress --config webpack/webpack.dev.js",
    "dev": "SET VERSION_APP=%npm_package_version%& SET NAME_APP=%npm_package_name%& webpack-dev-server --progress --config webpack/webpack.dev.js",
    "lint:check": "eslint src/** .",
    "lint:fix": "eslint . --fix",
    "style:check": "stylelint src/assets/styles/**/*.scss",
    "style:fix": "stylelint src/assets/styles/**/*.scss --fix",
    "prettier:check": "prettier --c .",
    "prettier:fix": "prettier --write .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fercho0/Profile.git"
  },
  "keywords": [
    "React",
    "Webpack5",
    "Redux",
    "Material-UI",
    "Firebase"
  ],
  "author": "Fernando.lc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fercho0/Profile/issues"
  },
  "homepage": "https://github.com/fercho0/Profile#readme",
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-react": "7.14.5",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.3",
    "compression-webpack-plugin": "9.2.0",
    "css-loader": "6.4.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.2.0",
    "eslint-plugin-react": "7.28.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.0",
    "lint-staged": "12.3.2",
    "mini-css-extract-plugin": "2.5.3",
    "prettier": "2.5.1",
    "sass": "1.49.7",
    "sass-loader": "12.4.0",
    "standard": "16.0.4",
    "style-loader": "3.3.1",
    "stylelint": "14.3.0",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard-scss": "3.0.0",
    "webpack": "5.59.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.3.1",
    "webpack-merge": "5.8.0",
    "webpackbar": "5.0.0-3"
  },
  "dependencies": {
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@mui/icons-material": "5.4.2",
    "@mui/material": "5.4.1",
    "@mui/lab": "5.0.0-alpha.69",
    "@mui/styles": "5.4.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-router": "6.2.1",
    "react-router-dom": "6.2.1",
    "redux": "4.1.2",
